{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "no-bitwise": [0],
    "max-len": [0],
    "react/jsx-props-no-spreading": [0],
    "import/prefer-default-export": [0],
    "no-alert": [0],
    "linebreak-style": [0]
  }
}
